{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "build/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "public/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "public/sitemap.xml"
    },
    {
      "src": "^/$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod"
    },
    {
      "src": "^/kileshwar-mahadev-ji-neemuch$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/kileshwar-mahadev-ji-neemuch"
    },
    {
      "src": "^/sanwaliya-ji-mandir-bhadsoda$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/sanwaliya-ji-mandir-bhadsoda"
    },
    {
      "src": "^/sanjay-case-study$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/sanjay-case-study"
    },
    {
      "src": "^/lic-case-study$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/lic-case-study"
    },
    {
      "src": "^/lic-neemuch-srs$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/lic-neemuch-srs"
    },
    {
      "src": "^/zedemy-case-study$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/zedemy-case-study"
    },
    {
      "src": "^/eventease-case-study$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/eventease-case-study"
    },
    {
      "src": "^/connectnow-case-study$",
      "dest": "https://gj48940cgb.execute-api.ap-south-1.amazonaws.com/prod/connectnow-case-study"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; connect-src 'self' https://gj48940cgb.execute-api.ap-south-1.amazonaws.com; script-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https://sanjay-patidar.vercel.app https://streetviewpixels-pa.googleapis.com https://d12uvtgcxr5qif.cloudfront.net https://maps.googleapis.com https://cdn.svgporn.com https://raw.githubusercontent.com https://d1.awsstatic.com; font-src 'self' data:; frame-src 'self' https://*.google.com https://streetviewpixels-pa.googleapis.com;"
        }
      ]
    }
  ]
}
